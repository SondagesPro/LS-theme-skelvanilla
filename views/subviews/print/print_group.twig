<div class="survey-group {{odd_even}}">
    <h2>{{group.name}}</h2>
    <div class="group-desc">
        {{group.description}}
    </div>
    {% for question in group.questions %}
        {% set questionTemplate = './subviews/print/print_question.twig' %}
        {{ include(questionTemplate) }}
    {% endfor %}
</div>
